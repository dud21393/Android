<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jsy.wordapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WriteActivity"></activity>

        <activity android:name=".VocabularyActivity"></activity>
        <activity android:name=".VocabularyList"></activity>
        <activity android:name=".WordList"></activity>
        <activity android:name=".WordAdd"></activity>
        <activity android:name=".WordTest"></activity>
    </application>

</manifest>